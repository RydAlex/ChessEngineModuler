        <class>AMQPManagment.DatabaseConnections.Entities.Score</class>
        <class>AMQPManagment.DatabaseConnections.Entities.CurrentElo</class>
        <class>AMQPManagment.DatabaseConnections.Entities.DepthWith2MoreEloUseWinLose</class>
        <class>AMQPManagment.DatabaseConnections.Entities.EloGamesHistory</class>
        <class>AMQPManagment.DatabaseConnections.Entities.EngineName</class>
        <properties>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
